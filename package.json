{
	"name": "lunarlib",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch",
		"debug": "nest start --debug --watch",
		"prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"typeorm": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d dist/databases/configs/typeorm.config.js",
		"db:migrate:run": "yarn build && yarn typeorm migration:run",
		"db:migrate:generate": "yarn build && sh -c 'yarn typeorm migration:generate ./src/databases/migrations/$0'",
		"db:migrate:revert": "yarn typeorm migration:revert",
		"db:seed": "yarn build -b tsc && ts-node -r tsconfig-paths/register node_modules/typeorm-extension/bin/cli.cjs seed:run -d dist/src/databases/configs/seed-typeorm.config.js"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.749.0",
		"@aws-sdk/s3-request-presigner": "^3.749.0",
		"@keyv/redis": "^4.3.2",
		"@nestjs/axios": "^4.0.0",
		"@nestjs/bull": "^11.0.2",
		"@nestjs/cache-manager": "^3.0.0",
		"@nestjs/common": "^11.0.1",
		"@nestjs/config": "^4.0.1",
		"@nestjs/core": "^11.0.1",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/mapped-types": "^2.1.0",
		"@nestjs/microservices": "^11.0.11",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^11.0.1",
		"@nestjs/platform-socket.io": "^11.0.11",
		"@nestjs/throttler": "^6.4.0",
		"@nestjs/typeorm": "^11.0.0",
		"@nestjs/websockets": "^11.0.11",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.5",
		"axios": "^1.8.3",
		"bcrypt": "^5.1.1",
		"bull": "^4.16.5",
		"cache-manager": "^6.4.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"dotenv": "^16.4.7",
		"dotenv-expand": "^12.0.1",
		"firebase-admin": "^13.2.0",
		"husky": "^9.1.7",
		"kysely": "^0.27.6",
		"mime": "4.0.6",
		"moment": "^2.30.1",
		"moment-timezone": "^0.5.47",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"pg": "^8.14.1",
		"postgres": "^3.4.5",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"tsx": "^4.19.3",
		"typeorm": "^0.3.21",
		"typeorm-extension": "^3.7.0",
		"typeorm-naming-strategies": "^4.1.0",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@faker-js/faker": "^9.6.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.11.11",
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.13.10",
		"@types/passport-jwt": "^4.0.1",
		"@types/pg": "^8.11.11",
		"@types/supertest": "^6.0.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"globals": "^15.14.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"run-script-webpack-plugin": "^0.2.2",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"swc-loader": "^0.2.6",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.20.0",
		"webpack": "^5.98.0",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
